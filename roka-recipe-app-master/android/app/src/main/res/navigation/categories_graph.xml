<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/categoriesGraph"
    app:startDestination="@id/categoriesFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.fabirt.roka.features.categories.presentation.view.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoriesFragment_to_categoryDetailFragment"
            app:destination="@id/categoryDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailFragment"
        android:name="com.fabirt.roka.features.categories.presentation.view.CategoryDetailFragment"
        android:label="fragment_category_detail"
        tools:layout="@layout/fragment_category_detail" >
        <argument
            android:name="category"
            app:argType="com.fabirt.roka.features.categories.domain.model.CategoryItem" />
    </fragment>
</navigation>